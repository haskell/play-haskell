cabal-version:       2.0
name:                sandbox
version:             0.1.0.0
build-type:          Simple

#ifdef AS_EXECUTABLE
executable thing
  main-is: Main.hs
#else
library thing
  exposed-modules: Main
#endif

  build-depends:
    base,
    aeson,
    assoc,
    async,
    attoparsec,
    base16,
    base64,
    bifunctors,
    comonad,
    contravariant,
    distributive,
    dlist,
    effectful,
    effectful-core,
    generically,
    hashable,
    indexed-traversable,
    integer-logarithms,
    monad-control,
    primitive,
    QuickCheck,
    random,
    safe-exceptions,
    scientific,
    semialign,
    semigroupoids,
    strict,
    text-short,
    th-abstraction,
    these,
    transformers,
    transformers-base,
    unliftio,
    unliftio-core,
    unordered-containers,
    uuid-types,
    vector,
    vector-stream,
    witherable
  hs-source-dirs:      .
  default-language:    Haskell2010
  ghc-options:         -Wall -rtsopts
